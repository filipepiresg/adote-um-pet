{
  "name": "adotepet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev:android": "react-native run-android",
    "dev:ios": "cd ios; pod install; cd ..; react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew clean && ./gradlew assembleRelease ",
    "lint": "eslint --ext .js ./src",
    "pretty": "prettier --write",
    "android:reconnect": "adb reverse tcp:8081 tcp:8081",
    "android:demo-mode": "adb shell settings put global sysui_demo_allowed 1 && adb shell am broadcast -a com.android.systemui.demo -e command clock -e hhmm 1200 && adb shell am broadcast -a com.android.systemui.demo -e command network -e mobile show -e level 4 -e datatype false && adb shell am broadcast -a com.android.systemui.demo -e command notifications -e visible false && adb shell am broadcast -a com.android.systemui.demo -e command battery -e plugged false -e level 100",
    "android:demo-mode-off": "adb shell am broadcast -a com.android.systemui.demo -e command exit",
    "connect-debugger": "adb reverse tcp:9090 tcp:9090"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/analytics": "^11.1.1",
    "@react-native-firebase/app": "^11.1.1",
    "@react-native-firebase/auth": "^11.1.1",
    "@react-native-firebase/firestore": "^11.1.1",
    "@react-native-firebase/storage": "^11.1.1",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/drawer": "^5.12.4",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "date-fns": "^2.19.0",
    "formik": "^2.2.6",
    "lodash": "^4.17.21",
    "metro-config": "^0.65.2",
    "polished": "^4.1.1",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-actionsheet": "^2.4.2",
    "react-native-config": "^1.4.2",
    "react-native-geocoding": "^0.5.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-get-random-values": "^1.6.0",
    "react-native-image-picker": "^3.3.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-maps": "0.27.1",
    "react-native-masked-text": "^1.13.0",
    "react-native-onesignal": "^4.0.5",
    "react-native-permissions": "^3.0.1",
    "react-native-reanimated": "^2.0.0",
    "react-native-root-siblings": "^4.1.0",
    "react-native-root-toast": "^3.2.1",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^8.1.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-community/eslint-plugin": "^1.1.0",
    "@types/styled-components-react-native": "^5.1.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "eslint": "^7.14.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "17.0.1",
    "reactotron-react-native": "^5.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "./node_modules/.bin/prettier --write",
      "./node_modules/.bin/eslint --fix"
    ]
  }
}
